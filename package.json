{
    "name": "close-to-the-left",
    "displayName": "close to the left",
    "description": "simply adds 'Close to the Left' item to the Title Tab menu",
    "version": "0.0.1",
    "publisher": "ctf0",
    "homepage": "https://github.com/ctf0/close-to-the-left/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ctf0/close-to-the-left.git"
    },
    "bugs": {
        "url": "https://github.com/ctf0/close-to-the-left/issues"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "close",
        "left",
        "tabs",
        "editor"
    ],
    "icon": "icon.png",
    "contributes": {
        "menus": {
            "editor/context": [
                {
                    "command": "workbench.action.closeEditorsToTheLeft",
                    "group": "5_close"
                },
                {
                    "command": "workbench.action.closeEditorsToTheRight",
                    "group": "5_close"
                }
            ],
            "editor/title/context": [
                {
                    "command": "workbench.action.closeEditorsToTheLeft",
                    "group": "1_close"
                }
            ]
        }
    }
}
